#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if(n>0)
    {
        cout<<"Number is Positive"<<endl;
    }
    else if(n<0)
    {
        cout<<"Number is negative"<<endl;
        
    }
    else if(n==0)
    {
        cout<<"Number is 0"<<endl;
    }
    if(n%2==0)
    {
        cout<<"Number is even"<<endl;
    }
    else
    {
        cout<<"Number is odd"<<endl;
    }
}
